    "postcss": "8.4.21",
    "postcss-import": "14.0.2",
    "postcss-loader": "7.3.3",
    "rollup": "2.45.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.0"
  }
}
